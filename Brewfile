# Add Cask to Homebrew formulae
tap "homebrew/bundle"
tap "homebrew/cask"
tap "homebrew/cask-fonts"
tap "homebrew/core"
tap "homebrew/services"

# Programs
brew "git"
brew "node"
brew "pnpm"
brew "postgresql", restart_service: true

